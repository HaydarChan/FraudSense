VITE_API_BASE_URL=http://localhost:3001

# Optional: Enable Twilio STUN in addition to Google STUN (off by default)
# Set to '1' to enable
VITE_USE_TWILIO_STUN=0

# Optional: TURN server fallback (recommended for NAT traversal)
# These are fallback values if backend TURN config fails
# VITE_TURN_SERVER=your-server-ip-or-domain.com
# VITE_TURN_USER=fraudsense
# VITE_TURN_SECRET=fraudsense123

# Force using TURN relay only (for diagnostics). Set to '1' to force.
VITE_ICE_RELAY_ONLY=0
