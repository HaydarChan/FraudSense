{
  "name": "datathon-final",
  "version": "1.0.0",
  "description": "Real-time chat application with fraud detection",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd front-end-new && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "install:all": "npm install && cd front-end-new && npm install && cd ../backend && npm install",
    "build": "cd front-end-new && npm run build",
    "start": "cd backend && npm start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "chat",
    "websocket",
    "fraud-detection",
    "nextjs",
    "socket.io"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "socket.io-client": "^4.8.1"
  }
}
